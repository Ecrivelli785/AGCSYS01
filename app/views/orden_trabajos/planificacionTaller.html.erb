<div class="d-flex justify-content-center">
  <a class=" btn btn-warning text-center my-3" type="button" href="/planificacionTallerPDF.pdf"><h4><strong>DESCARGAR PDF</strong></h4> </a>
</div>
<div class="d-flex justify-content-around">
  <div class="container">
      <table class="table table-striped">
        <h1 class="mt-3 p-1 bg-danger text-white"><strong>SM en espera</strong></h1>
      <thead>
        <tr>
          <th>Cliente</th>
          <th>Producto</th>
        </tr>
      </thead>
      <tbody>
    <% @orden_trabajos.each do |orden_trabajo| %>
       <% if cadenatovector(orden_trabajo.procesos).include? "SM" and orden_trabajo.estado_actual != "Impresión" and orden_trabajo.estado_actual != "Pendiente" and orden_trabajo.estado_actual != "Terminación" and orden_trabajo.estado_actual != "Empaque" and cadenatovector(orden_trabajo.procesos) != "PM"%>
          <tr>
            <td><%= orden_trabajo.clinom %></td>
            <td><%= orden_trabajo.nomprod.first(20) %></td>
            <td>
        <% end %>
      <% end %>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="container">
      <table class="table table-striped">
        <h1 class="mt-3 p-1 bg-success text-white"><strong>SM LISTO</strong></h1>
